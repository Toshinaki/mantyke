{
  "name": "mantyke-docs",
  "root": "apps/docs",
  "sourceRoot": "apps/docs",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/.next", "{projectRoot}/out"],
      "options": {
        "command": "pnpm run docs:build",
        "cwd": "{workspaceRoot}"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run dev",
        "cwd": "{workspaceRoot}"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/docs && pnpm run typecheck",
        "cwd": "{workspaceRoot}"
      }
    }
  }
}
